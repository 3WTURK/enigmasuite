; SIP Configuration

[general]
bindaddr=::
transport=tcp
tcpenable=yes
canreinvite=no
jbenable=yes
jbforce=yes

; stable values when net is stable
; jbmaxsize=25
; jbresyncthreshold=200

; values for unstable network
jbmaxsize=100
jbresyncthreshold=300



; my own phone

[100]
type=peer
qualify=yes
secret=100
host=dynamic
context=friends
transport=udp



; my addresses

[basic-options](!)
    type=peer
    qualify=5000
    qualifyfreq=5
    context=incoming
<% if addresses.class == Array then %><% addresses.each do |address| %>
[<%= address["name"] %>](basic-options)
    host=<%= address["ipv6"] %>
    callerid="<%= address["name"] %> <<%= address["name"] %>>"
<% end %><% end %>
