
# docs: http://wiki.ubuntuusers.de/WLAN/wpa_supplicant

ctrl_interface=/var/run/wpa_supplicant
eapol_version=1
ap_scan=1

network={
    ssid="<%= wlan_ssid %>"
    scan_ssid=1
    proto=RSN
    key_mgmt=WPA-PSK
    pairwise=TKIP
    group=TKIP
    #pairwise=CCMP
    #group=CCMP
    psk="<%= wlan_pass %>"
}

